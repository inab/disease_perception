[
	{
		"selector": "node",
		"css": {
			"content": "data(name)",
			"text-valign": "center",
			"text-halign": "center",
			"text-wrap": "wrap",
			"background-color": "data(color)",
			"font-size": "0.7em",
			"text-outline-color": "#555",
			"text-outline-width": "1px",
			"color": "#fff",
			"overlay-padding": "6px",
			"opacity": "1",
			"z-index": "10"
		}
	},
	{
		"selector": "node:selected",
		"css": {
			"border-width": "6px",
			"border-color": "#AAD8FF",
			"border-opacity": "0.5",
			"text-outline-color": "#77828C"
		}
	},
{
  "selector": "node.highlighted",
  "style": {
    "background-color": "#394855",
    "text-outline-color": "#394855"
  }
},
{
  "selector": "edge.filtered",
  "style": {
    "opacity": "0"
  }
},
	{
		"selector": "node.D",
		"css": {
			"content": "data(label)",
			"padding": "0.25em",
			"width": "label",
			"height": "label",
			"shape": "roundrectangle"
		}
	},
	{
		"selector": "node.PSG",
		"css": {
			"content": "data(label)",
			"shape": "diamond",
			"text-outline-color": "#000",
			"width": "mapData(size,1,10,20,60)",
			"height": "mapData(size,1,10,20,60)"
		}
	},
	{
		"selector": "node.Dr",
		"css": {
			"shape": "ellipse",
			"width": "label",
			"height": "label"
		}
	},
	{
		"selector": "node.G",
		"css": {
			"shape": "hexagon",
			"width": "label",
			"height": "label"
		}
	},
	{
		"selector": "node.P",
		"css": {
			"shape": "triangle",
			"width": "label",
			"height": "label"
		}
	},
	{
		"selector": "$node > node",
		"css": {
			"content": "data(groupname)",
			"text-wrap": "wrap",
			"padding-top": "10px",
			"padding-left": "10px",
			"padding-bottom": "10px",
			"padding-right": "10px",
			"text-valign": "top",
			"text-halign": "center",
			"border-color": "data(color)",
			"background-opacity": "0"
		}
	},
	{
		"selector": "edge",
		"css": {
			"haystack-radius": "0.5",
			"opacity": "0.4",
			"overlay-padding": "3px"
		}
	},
	{
		"selector": "edge:selected",
		"css": {
			"background-color": "black",
			"line-color": "black",
			"target-arrow-color": "black",
			"source-arrow-color": "black"
		}
	},
	{
		"selector": "edge.highlighted",
		"css": {
			"color": "#7F7F7F"
		}
	},
	{
		"selector": "edge.CM",
		"css": {
			"curve-style": "unbundled-bezier",
			"haystack-radius": "0.5",
			"opacity": "0.4",
			"width": "mapData(abs_rel_risk, 0, 50, 1, 8)",
			"overlay-padding": "3px"
		}
	},
	{
		"selector": "edge.CMp",
		"css": {
			"target-arrow-shape": "triangle",
			"line-color": "mapData(rel_risk, 0, 20, rgb(127,127,255), blue)",
			"target-arrow-color": "mapData(rel_risk, 0, 20, rgb(127,127,255), blue)"
		}
	},
	{
		"selector": "edge.CMn",
		"css": {
			"target-arrow-shape": "tee",
			"line-color": "mapData(rel_risk, -20, 0, red, rgb(255,127,127))",
			"target-arrow-color": "mapData(rel_risk, -20, 20, red, rgb(255,127,127))"
		}
	},
	{
		"selector": "edge.confirmed",
		"css": {
			"overlay-color": "yellow",
			"overlay-opacity": "0.2",
			"overlay-padding": "mapData(abs_rel_risk, 0, 50, 2, 9)"
		}
	},
	{
		"selector": "edge.GI",
		"css": {
			"curve-style": "bezier",
			"haystack-radius": "0.5",
			"opacity": "0.3",
			"line-color": "mapData(sign, -1, 1, green, orange)",
			"target-arrow-color": "mapData(sign, -1, 1, green, orange)",
			"width": "1",
			"overlay-padding": "3px"
		}
	},
	{
		"selector": "edge.DI",
		"css": {
			"curve-style": "bezier",
			"haystack-radius": "0.5",
			"opacity": "0.3",
			"line-color": "mapData(sign, -1, 1, lightgray, gray)",
			"target-arrow-color": "mapData(sign, -1, 1, lightgray, gray)",
			"width": "1",
			"overlay-padding": "3px"
		}
	}
]
